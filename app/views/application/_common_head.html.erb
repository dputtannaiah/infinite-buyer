<meta http-equiv="content-type" content="text/html"/>
<meta name="keywords" content="<%= meta_keywords %>"/>
<meta name="description" content="<%= meta_description %>"/>
<meta property="og:image" content="<%= "#{site_address_with_protocol}/images/favicon-72x72.png" %>"/>
<meta property="og:site_name" content="<%= configatron.site_name %>"/>
<meta property="og:title" content="Shop Online at Infinite Buyer"/>
<meta property="og:url" content="<%= site_address_with_protocol %>"/>
<%= favicon_link_tag '/images/favicon-72x72.png', :rel => 'icon', :type => 'image/png' %>
<%= favicon_link_tag '/images/favicon-72x72.png', :rel => 'shortcut icon', :type => 'image/png' %>
<%= favicon_link_tag '/images/favicon-32x32.png', :rel => 'icon', :type => 'image/png' %>
<%= favicon_link_tag '/images/favicon-32x32.png', :rel => 'shortcut icon', :type => 'image/png' %>
<%= content_tag :title, browser_title %>

