<script type='text/javascript'>
    //Start LogOut Facebook
    (function () {

        jQuery(function () {
            $('body').prepend('<div id="fb-root"></div>');
            return $.ajax({
                url:"" + window.location.protocol + "//connect.facebook.net/en_US/all.js",
                dataType:'script',
                cache:true
            });
        });

        window.fbAsyncInit = function () {
            FB.init({
                appId:'<%= configatron.omniAuth.facebook.api_key %>',
                cookie:true
            });
            return $('#dev_facebook_sign_out').click(function (e) {
                FB.getLoginStatus(function (response) {
                    if (response.authResponse) {
                        return FB.logout();
                    }
                });
                return true;
            });
        };

    }).call(this);
    //End LogOut Facebook
</script>