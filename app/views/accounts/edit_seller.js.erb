<% if @account.present? %>
  jQuery('div.ib-bs-registration ul li a').removeClass('active');
  jQuery('div.ib-bs-registration ul li a').eq(1).addClass('active');
  jQuery('div.ib-buyer-account-form').html('<%= escape_javascript(render :partial => 'seller') %>');
<% else %>
  jQuery('div.ib-buyer-account-form').html('<%= escape_javascript(render :partial => 'alert_message', :locals => {:type => "seller"}) %>');
<% end %>
